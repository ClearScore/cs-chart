{
  "name": "cs-charts",
  "version": "0.0.0",
  "description": "Lightweight opinionated chart library (line, donut and bar charts)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "release": "standard-version && git push --follow-tags",
    "postrelease": "conventional-github-releaser"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-es2015-modules-umd",
      "transform-es2015-spread",
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClearScore/cs-charts.git"
  },
  "author": "Neil Barton <neil@roughcoder.com> (http://www.roughcoder.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ClearScore/cs-charts/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "6.24.1",
    "conventional-github-releaser": "^1.1.3",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "express": "^4.15.3",
    "invariant": "^2.2.2",
    "serve-static": "^1.12.3",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "2.4.1",
    "yargs": "^8.0.1",
    "standard-version": "^4.0.0"
  },
  "homepage": "https://github.com/ClearScore/cs-charts#readme",
  "dependencies": {
  }
}
